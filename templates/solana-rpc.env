{% if validator_block_engine_url is defined and
    validator_relayer_url is defined and
    validator_shred_receiver_address is defined 
%}
BLOCK_ENGINE_URL="{{ validator_block_engine_url }}"
RELAYER_URL="{{ validator_relayer_url }}"
SHRED_RECEIVER_ADDRESS="{{ validator_shred_receiver_address }}"
{% endif %}
{% if validator_bigtable_enabled %}
GOOGLE_APPLICATION_CREDENTIALS=/home/{{ validator_username }}/bigtable.json
{% endif %}
{% for env_v in validator_environment %}
{{ env_v }}
{% endfor %}